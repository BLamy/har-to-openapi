{
  "name": "har-to-openapi",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Jonluca DeCaro <lottie@jonlu.ca>",
  "description": "Convert a HAR object to an OpenAPI object",
  "types": "dist/mjs/index.d.ts",
  "files": [
    "/dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "har",
    "openapi",
    "json",
    "api",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonluca/har-to-openapi.git"
  },
  "bugs": {
    "url": "https://github.com/jonluca/har-to-openapi/issues"
  },
  "homepage": "https://github.com/jonluca/har-to-openapi",
  "scripts": {
    "prepublish": "yarn build",
    "lint": "eslint src",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && node scripts/fixup.js",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "test": "vitest",
    "test:update": "vitest -u",
    "test:watch": "vitest -w",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@loopback/openapi-v3-types": "^1.2.1",
    "@types/har-format": "^1.2.8",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.6",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.0.15",
    "@types/sort-json": "^2.0.1",
    "browser-json-schema-to-openapi-schema": "^0.1.4",
    "js-yaml": "^4.1.0",
    "json-schema-deref-sync": "^0.14.0",
    "lodash-es": "^4.17.21",
    "openapi3-ts": "^2.0.2",
    "pluralize": "^8.0.0",
    "quicktype-core": "^6.0.71",
    "sort-json": "^2.0.1",
    "typescript": "4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/eslint": "8.4.5",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.0.3",
    "openapi-schema-validator": "^12.0.0",
    "prettier": "^2.7.1",
    "vitest": "^0.20.2"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ],
    "**/*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
